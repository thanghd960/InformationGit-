{
   "background": {
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "128": "icons/128.png",
         "16": "icons/16.png",
         "32": "icons/32.png",
         "48": "icons/48.png"
      },
      "default_title": "Fox Convertor"
   },
   "chrome_url_overrides": {
      "newtab": "newtab/index.html"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "*://*.fileconvertor.org/*" ],
      "run_at": "document_start"
   } ],
   "content_security_policy": "script-src https://*.srchweb.org 'self'; object-src 'self';",
   "description": "Convert DOC to PDF and PDF to DOC using Fox Convertor New tab",
   "icons": {
      "128": "icons/128.png",
      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png"
   },
   "key": "",
   "manifest_version": 2,
   "minimum_chrome_version": "32",
   "name": "Fox Convertor",
   "permissions": [ "\u003Call_urls>", "cookies", "storage", "tabs" ],
   "short_name": "foxconvertor",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.0.1.62617",
   "web_accessible_resources": [ "/icons/16.png" ]
}
