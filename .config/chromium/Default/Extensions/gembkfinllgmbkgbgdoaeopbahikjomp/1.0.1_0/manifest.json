{
   "background": {
      "persistent": false,
      "scripts": [ "background.js" ]
   },
   "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "index.html"
   },
   "content_scripts": [ {
      "js": [ "content.js" ],
      "matches": [ "*://*/*" ]
   } ],
